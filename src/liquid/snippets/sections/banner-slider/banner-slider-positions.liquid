{%- assign message_pos = block.settings.message_pos -%}
{%- assign sub_title_pos = block.settings.sub_title_pos -%}
{%- assign title_pos = block.settings.title_pos -%}

{%- comment -%}Position 1{%- endcomment -%}

{%- if title_pos == 1 -%}
  {%- render 'banner-slider-title' block: block -%}
{%- endif -%}

{%- if sub_title_pos == 1 -%}
  {%- render 'banner-slider-sub-title' block: block -%}
{%- endif -%}

{%- if message_pos == 1 -%}
  {%- render 'banner-slider-message' block: block -%}
{%- endif -%}

{%- if button_pos == 1 -%}
  {%- render 'banner-slider-button',
    block: block,
    text_alignment_desk: text_alignment_desk,
    text_alignment_phone: text_alignment_phone,
    text_alignment_tablet: text_alignment_tablet -%}
{%- endif -%}

{%- if custom_html_pos == 1 and custom_html != blank -%}
  {{- custom_html -}}
{%- endif -%}

